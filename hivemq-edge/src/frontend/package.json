{
  "name": "hivemq-edge-frontend",
  "private": true,
  "version": "0.0.30",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:prettier:write": "prettier --write .",
    "lint:stylelint": "stylelint './src/**/*.css'",
    "lint:all": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && prettier --check .",
    "dev:openAPI": "openapi --input '../../../../hivemq-edge/ext/hivemq-edge-openapi-2024.1.yaml' -o ./src/api/__generated__ -c axios --name HiveMqClient --exportSchemas true",
    "dev:chakra:types": "chakra-cli tokens './src/modules/Theme/themeHiveMQ.ts' --out 'node_modules/.pnpm/@chakra-ui+styled-system@2.9.1/node_modules/@chakra-ui/styled-system/dist/theming.types.d.ts'",
    "cypress:open": "cypress open",
    "cypress:open:component": "cypress open --component --browser chrome",
    "cypress:open:e2e": "cypress open --e2e --browser chrome",
    "cypress:run:component": "cypress run --component",
    "cypress:run:e2e": "cypress run --e2e",
    "cypress:run": "npx cypress run --e2e --browser chrome && npx cypress run --component --browser chrome",
    "cypress:percy": "LOCAL_NONCE=$(date '+%s') && PERCY_PARALLEL_NONCE=$LOCAL_NONCE percy exec --parallel -- cypress run --e2e && PERCY_PARALLEL_NONCE=$LOCAL_NONCE percy exec --parallel -- cypress run --component && PERCY_PARALLEL_NONCE=$LOCAL_NONCE percy build:finalize ",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.2",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/radio": "^2.1.2",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/skip-nav": "^2.1.0",
    "@chakra-ui/utils": "^2.0.14",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.12",
    "@monaco-editor/react": "^4.6.0",
    "@nivo/bar": "^0.84.0",
    "@nivo/core": "^0.84.0",
    "@nivo/line": "^0.84.0",
    "@rjsf/chakra-ui": "^5.18.1",
    "@rjsf/core": "^5.18.1",
    "@rjsf/utils": "^5.18.1",
    "@rjsf/validator-ajv8": "^5.18.1",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@tanstack/react-table": "^8.15.0",
    "@types/d3-array": "^3.2.1",
    "@types/d3-hierarchy": "^3.1.7",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.6.8",
    "chakra-react-select": "^4.7.6",
    "csstype": "^3.1.3",
    "d3-array": "^3.2.4",
    "d3-hierarchy": "^3.1.2",
    "elkjs": "^0.9.2",
    "form-data": "^4.0.0",
    "framer-motion": "^10.18.0",
    "i18next": "^22.5.1",
    "luxon": "^3.4.4",
    "mermaid": "^10.9.0",
    "protobufjs": "^7.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-i18next": "^12.3.1",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.3",
    "reactflow": "^11.10.4",
    "uuid": "^9.0.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@percy/cli": "^1.28.2",
    "@percy/cypress": "^3.1.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-c8": "^0.31.4",
    "@vitest/coverage-istanbul": "^0.32.4",
    "@vitest/ui": "^0.31.4",
    "axe-core": "^4.9.0",
    "cypress": "^12.17.4",
    "cypress-axe": "^1.5.0",
    "cypress-each": "^1.14.0",
    "cypress-terminal-report": "^5.3.12",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "i18next-pseudo": "^2.2.1",
    "jsdom": "^22.1.0",
    "msw": "^1.3.3",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "2.8.8",
    "sass": "^1.72.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.5.3",
    "vitest": "^0.33.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
