{
  "name": "hivemq-edge-frontend",
  "private": true,
  "version": "0.0.30",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:prettier:write": "prettier --write .",
    "lint:stylelint": "stylelint './src/**/*.css'",
    "lint:all": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && prettier --check .",
    "dev:openAPI": "openapi --input '../../../../hivemq-edge/ext/hivemq-edge-openapi-2023.10.yaml' -o ./src/api/__generated__ -c axios --name HiveMqClient --exportSchemas true",
    "dev:chakra:types": "chakra-cli tokens './src/modules/Theme/themeHiveMQ.ts' --out 'node_modules/.pnpm/@chakra-ui+styled-system@2.9.1/node_modules/@chakra-ui/styled-system/dist/theming.types.d.ts'",
    "cypress:open": "cypress open",
    "cypress:open:component": "cypress open --component --browser chrome",
    "cypress:open:e2e": "cypress open --e2e --browser chrome",
    "cypress:run:component": "cypress run --component",
    "cypress:run:e2e": "cypress run --e2e",
    "cypress:run": "npx cypress run --e2e --browser chrome && npx cypress run --component --browser chrome",
    "cypress:percy": "LOCAL_NONCE=$(date '+%s') && PERCY_PARALLEL_NONCE=$LOCAL_NONCE percy exec --parallel -- cypress run --e2e && PERCY_PARALLEL_NONCE=$LOCAL_NONCE percy exec --parallel -- cypress run --component && PERCY_PARALLEL_NONCE=$LOCAL_NONCE percy build:finalize ",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.1.1",
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/radio": "^2.0.22",
    "@chakra-ui/react": "^2.7.1",
    "@chakra-ui/skip-nav": "^2.0.15",
    "@chakra-ui/utils": "^2.0.14",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.3",
    "@monaco-editor/react": "^4.5.1",
    "@nivo/bar": "^0.84.0",
    "@nivo/core": "^0.84.0",
    "@nivo/line": "^0.84.0",
    "@rjsf/chakra-ui": "^5.15.1",
    "@rjsf/core": "^5.15.1",
    "@rjsf/utils": "^5.15.1",
    "@rjsf/validator-ajv8": "^5.15.1",
    "@tanstack/react-query": "^4.29.7",
    "@tanstack/react-query-devtools": "^4.29.7",
    "@tanstack/react-table": "^8.9.3",
    "@types/d3-array": "^3.0.7",
    "@types/d3-hierarchy": "^3.1.3",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.4.0",
    "chakra-react-select": "^4.7.6",
    "csstype": "^3.1.2",
    "d3-array": "^3.2.4",
    "d3-hierarchy": "^3.1.2",
    "form-data": "^4.0.0",
    "framer-motion": "^10.12.21",
    "i18next": "^22.4.15",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.3.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.11.2",
    "reactflow": "^11.8.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@percy/cli": "^1.24.2",
    "@percy/cypress": "^3.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/luxon": "^3.3.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.1",
    "@vitest/coverage-istanbul": "^0.32.4",
    "@vitest/ui": "^0.31.1",
    "axe-core": "^4.7.1",
    "cypress": "^12.13.0",
    "cypress-axe": "^1.4.0",
    "cypress-each": "^1.13.3",
    "cypress-terminal-report": "^5.1.1",
    "eslint": "^8.38.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "i18next-pseudo": "^2.2.1",
    "jsdom": "^22.0.0",
    "msw": "^1.2.1",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "2.8.8",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.33.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
