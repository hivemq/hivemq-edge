{
  "name": "hivemq-edge-frontend",
  "private": true,
  "version": "0.0.30",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint '{src}/**/*.{css}'",
    "lint:all": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && prettier --check .",
    "dev:openAPI": "openapi --input /Users/nicolas/IdeaProjects/hivemq-edge-test/src/test/resources/hivemq-edge-openapi.yaml -o ./src/api/__generated__ -c axios --name HiveMqClient --exportSchemas true",
    "cypress:open": "cypress open",
    "cypress:open:component": "cypress open --component --browser chrome",
    "cypress:open:e2e": "cypress open --e2e --browser chrome",
    "cypress:run:component": "cypress run --component",
    "cypress:run:e2e": "cypress run --e2e",
    "cypress:percy": "percy exec -- cypress run ",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "preview": "vite preview"
  },
