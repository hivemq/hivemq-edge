#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@vitest+ui@3.0.5_jsdom@24.0.0_msw@2.7.0_sass@1.70.0/node_modules/vitest/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@vitest+ui@3.0.5_jsdom@24.0.0_msw@2.7.0_sass@1.70.0/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@vitest+ui@3.0.5_jsdom@24.0.0_msw@2.7.0_sass@1.70.0/node_modules/vitest/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@vitest+ui@3.0.5_jsdom@24.0.0_msw@2.7.0_sass@1.70.0/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
