#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.2@xlsx-0.20.2.tgz/node_modules/xlsx/bin/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.2@xlsx-0.20.2.tgz/node_modules/xlsx/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.2@xlsx-0.20.2.tgz/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.2@xlsx-0.20.2.tgz/node_modules/xlsx/bin/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.2@xlsx-0.20.2.tgz/node_modules/xlsx/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@cdn.sheetjs.com+xlsx-0.20.2@xlsx-0.20.2.tgz/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../xlsx/bin/xlsx.njs" "$@"
else
  exec node  "$basedir/../xlsx/bin/xlsx.njs" "$@"
fi
