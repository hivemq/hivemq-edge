    "@chakra-ui/spinner": "2.1.0"
  },
  "devDependencies": {
    "framer-motion": "^10.12.16",
    "react-icons": "^4.2.0",
    "react-spinners": "0.11.0",
    "react": "^18.0.0",
    "clean-package": "2.2.0",
    "@chakra-ui/utils": "2.0.15",
    "@chakra-ui/system": "2.6.0",
    "@chakra-ui/icons": "2.1.0",
    "@chakra-ui/theme": "3.2.0",
    "@chakra-ui/layout": "2.3.0"
  },
  "peerDependencies": {
