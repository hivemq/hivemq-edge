#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@percy+cli-command@1.28.9_typescript@5.7.3/node_modules/@percy/cli-command/bin/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@percy+cli-command@1.28.9_typescript@5.7.3/node_modules/@percy/cli-command/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@percy+cli-command@1.28.9_typescript@5.7.3/node_modules/@percy/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@percy+cli-command@1.28.9_typescript@5.7.3/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@percy+cli-command@1.28.9_typescript@5.7.3/node_modules/@percy/cli-command/bin/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@percy+cli-command@1.28.9_typescript@5.7.3/node_modules/@percy/cli-command/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@percy+cli-command@1.28.9_typescript@5.7.3/node_modules/@percy/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/@percy+cli-command@1.28.9_typescript@5.7.3/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@percy+cli-command@1.28.9_typescript@5.7.3/node_modules/@percy/cli-command/bin/readme.js" "$@"
else
  exec node  "$basedir/../../../../../../@percy+cli-command@1.28.9_typescript@5.7.3/node_modules/@percy/cli-command/bin/readme.js" "$@"
fi
