#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/terser@5.37.0/node_modules/terser/bin/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/terser@5.37.0/node_modules/terser/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/terser@5.37.0/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/terser@5.37.0/node_modules/terser/bin/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/terser@5.37.0/node_modules/terser/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/terser@5.37.0/node_modules:/Users/anthony/Projects/hivemq/hivemq-edge/hivemq-edge/src/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
